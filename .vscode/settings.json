{
  "files.exclude": {
    "**/.git": true,
    "**/node_modules": true
  },
  "editor.formatOnSave": true,
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "editor.tabSize": 2,
  "tslint.autoFixOnSave": true,
  "typescript.preferences.quoteStyle": "double",
  "typescript.format.semicolons": "remove"
}
