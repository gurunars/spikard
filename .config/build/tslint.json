{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "no-redundant-jsdoc": true,
    "class-name": true,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-var-requires": true,
    "no-unnecessary-callback-wrapper": true,
    "align": [
      true,
      "parameters",
      "arguments",
      "statements"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "object-literal-sort-keys": false,
    "ban": false,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "arrow-return-shorthand": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "only-arrow-functions": true,
    "promise-function-async": true,
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "label-position": true,
    "max-line-length": [
      true,
      120
    ],
    "no-var-keyword": true,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": false,
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "radix": true,
    "semicolon": [
      true,
      "never"
    ],
    "switch-default": true,
    "trailing-comma": [
      false
    ],
    "await-promise": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "ban-types": [
      true,
      []
    ]
  }
}